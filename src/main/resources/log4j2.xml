<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
	<Appenders>
		<Console name="Console">
			<PatternLayout
				pattern="[%d{dd-MM-yyyy HH:mm:ss.SSS}] [%t] [%p] %C{1}:%L - %m%n"/>
		</Console>

	</Appenders>

	<Loggers>

		<Root level="debug">
			<AppenderRef ref="Console" />
		</Root>
	</Loggers>

</Configuration>
